generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Facture {
  id Int @id @default(autoincrement())
  structure String?
  designation String[]
  quantity String[]
  nature String[]
  prixU String[]
  prixT String[]
  createdAt DateTime  @default(now()) 
}

model Employee {
  id String   @id @default(cuid())
  name String
  email String?
  tel String
  adresse String
  poste String
  salaire Int
}